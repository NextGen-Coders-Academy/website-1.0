<%- include('../partials/head') %>
<h1 class="font-outfit md:font-audiowide text-3xl pt-6 text-center pb-4">Upcoming Cohorts</h1>
<div class="flex align-center justify-center">
    <% courses.forEach(function(course) { %>
        <p><%= course.name %></p>
        <a class="button" href="/courses/<%= course.id %>">Go To Course!</a>
    <% }) %>
</div>
